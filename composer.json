{
    "name": "mindtwo/laravel-document-generator",
    "type": "proprietary",
    "description": "A project to define and generate documents.",
    "license": "MIT",
    "keywords": [
        "framework",
        "laravel"
    ],
    "authors": [
        {
            "name": "mindtwo GmbH",
            "email": "info@mindtwo.de",
            "homepage": "https://mindtwo.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "mindtwo/laravel-auto-create-uuid": "^2.6",
        "dompdf/dompdf": "^2.0.4",
        "ramsey/uuid": "^4.7",
        "laravel/framework": "^9.0|^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "pestphp/pest": "^2.34",
        "orchestra/testbench": "^8.21"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "mindtwo\\DocumentGenerator\\": "src/",
            "Tests\\": "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "mindtwo\\DocumentGenerator\\Providers\\DocumentGeneratorProvider"
            ],
            "aliases": {}
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
